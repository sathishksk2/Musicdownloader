            "required": true
        },
        "LOG_CHANNEL": {
            "description": "ID of a Channel which you want to RECEIVE LOGS.",
            "value": "",
            "required": true
        }
    },
    "buildpacks": [
        {
           "url": "heroku/python"
        },
        {
